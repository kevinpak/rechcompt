{"ast":null,"code":"import { Fragment as _Fragment } from \"react/jsx-runtime\";\nimport { jsx as _jsx } from \"react/jsx-runtime\";\nimport { useState, useEffect } from 'react';\nimport styles from './../../styles/components/go-up-the-page.module.scss';\n\nvar GoUpThePage = function GoUpThePage() {\n  var _useState = useState(false),\n      statusAnchor = _useState[0],\n      setStatusAnchor = _useState[1];\n\n  var goUp = function goUp() {\n    window.scrollTo(0, 0);\n  };\n\n  useEffect(function () {\n    window.onscroll = function (e) {\n      var scrollPosition = window.pageYOffset;\n\n      if (scrollPosition > 350) {\n        setStatusAnchor(true);\n      } else {\n        setStatusAnchor(false);\n      }\n    };\n  }, [statusAnchor]);\n  return /*#__PURE__*/_jsx(_Fragment, {\n    children: statusAnchor && /*#__PURE__*/_jsx(\"button\", {\n      className: styles.btn + ' ',\n      onClick: function onClick() {\n        return goUp();\n      },\n      children: /*#__PURE__*/_jsx(\"span\", {\n        className: styles.icon + ' icon-arrow-up icon-inner'\n      })\n    })\n  });\n};\n\nexport default GoUpThePage;","map":null,"metadata":{},"sourceType":"module"}